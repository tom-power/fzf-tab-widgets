local _fzfTabCompleteRecentDirs() {
  dirstackExisting=()
  for dir in "${dirstack[@]}"; do
    [[ -d "$dir" ]] && dirstackExisting+=("$dir")
  done
  compadd -a -f dirstackExisting
}

local _fzfTabCompleteFind() {
  IFS=$'\n'
  dirs=($(find . -not -path '*/.*' -mindepth 1 | head -10000))
  unset IFS
  compadd -a dirs
}

local _fzfTabCompleteBookmarks() {
  compadd -a -f bookmarks
}

_fzfTabComplete() {
  if [[ -v fzfTabCompleteBookmarks ]]; then
    _fzfTabCompleteBookmarks
  elif [[ -v fzfTabCompleteRecentDirs ]]; then
    _fzfTabCompleteRecentDirs
  elif [[ -v fzfTabCompleteFind ]]; then
    _fzfTabCompleteFind
  else
    $1
  fi
}
